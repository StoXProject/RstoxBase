{
    
    "DefineStrata": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "string"
    },

    "DefineAcousticPSU": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {
            "Stratum_PSU": {
                "type": "array",
                "items": [ 
                    {
                        "type": "object",
                        "properties": {
                            "Stratum": { "type": "string" },
                            "PSU": { "type": "string" }
                        },
                        "required": [
                            "Stratum",
                            "PSU"
                        ]
                    }
                ]
            },
            "EDSU_PSU": {
                "type": "array",
                "items": [ 
                    {
                        "type": "object",
                        "properties": {
                            "EDSU": { "type": "string" },
                            "PSU": { "type": "string" }
                        },
                        "required": [
                            "EDSU",
                            "PSU"
                        ]
                    }
                ]
            }
        },
        "required": [
            "Stratum_PSU",
            "EDSU_PSU"
        ]
    }, 
    
    "DefineSweptAreaPSU": {

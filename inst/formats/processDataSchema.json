					"Stratum",
					"PSU",
					"Layer",
					"Haul",
					"WeightingFactor"
				]
			}
		]
	}, 

	"Survey": {
		"$schema": "https://json-schema.org/draft/2020-12/schema",
		"type": "array",
		"items": [
			{
				"type": "object",
				"properties": {
					"Stratum": { "type": "string" },
					"Survey": { "type": "string" }
				},
				"required": [
					"Stratum",
					"Survey"
				]
			}
		]
	}, 
	
    "StratumPolygon": {
		"$schema": "https://json-schema.org/draft/2020-12/schema",
		"$id": "https://geojson.org/schema/FeatureCollection.json",
		"title": "GeoJSON FeatureCollection",
		"type": "object",
		"required": [
			"type",
			"features"
		],
		"properties": {
			"type": {
				"type": "string",
				"enum": [
					"FeatureCollection"
				]
			},
			"features": {
				"type": "array",
				"items": {
					"title": "GeoJSON Feature",
					"type": "object",
					"required": [
						"type",
						"properties",
						"geometry"
					],
					"properties": {
						"type": {
							"type": "string",
							"enum": [
								"Feature"
							]
						},
						"properties": {
							"oneOf": [
								{
									"type": "null"
								},
								{
									"type": "object"
								}
							]
						},
						"geometry": {
							"oneOf": [
								{
									"type": "null"
								},
								{
									"title": "GeoJSON Polygon",
									"type": "object",
									"required": [
										"type",
										"coordinates"
									],
									"properties": {
										"type": {
											"type": "string",
											"enum": [
												"Polygon"
											]
										},
										"coordinates": {
											"type": "array",
											"items": {
												"type": "array",
												"minItems": 4,
												"items": {
													"type": "array",
													"minItems": 2,
													"items": {
														"type": "number"
													}
												}
											}
										},
										"bbox": {
											"type": "array",
											"minItems": 4,
											"items": {
												"type": "number"
											}
										}
									}
								},
								{
									"title": "GeoJSON MultiPolygon",
									"type": "object",
									"required": [
										"type",
										"coordinates"
									],
									"properties": {
										"type": {
											"type": "string",
											"enum": [
												"MultiPolygon"
											]
										},
										"coordinates": {
											"type": "array",
											"items": {
												"type": "array",
												"items": {
													"type": "array",
													"minItems": 4,
													"items": {
														"type": "array",
														"minItems": 2,
														"items": {
															"type": "number"
														}
													}
												}
											}
										},
										"bbox": {
											"type": "array",
											"minItems": 4,
											"items": {
												"type": "number"
											}
										}
									}
								},
								{
									"title": "GeoJSON GeometryCollection",
									"type": "object",
									"required": [
										"type",
										"geometries"
									],
									"properties": {
										"type": {
											"type": "string",
											"enum": [
												"GeometryCollection"
											]
										},
										"geometries": {
											"type": "array",
											"items": {
												"oneOf": [
													{
														"title": "GeoJSON Polygon",
														"type": "object",
														"required": [
															"type",
															"coordinates"
														],
														"properties": {
															"type": {
																"type": "string",
																"enum": [
																	"Polygon"
																]
															},
															"coordinates": {
																"type": "array",
																"items": {
																	"type": "array",
																	"minItems": 4,
																	"items": {
																		"type": "array",
																		"minItems": 2,
																		"items": {
																			"type": "number"
																		}
																	}
																}
															},
															"bbox": {
																"type": "array",
																"minItems": 4,
																"items": {
																	"type": "number"
																}
															}
														}
													},
													{
														"title": "GeoJSON MultiPolygon",
														"type": "object",
														"required": [
															"type",
															"coordinates"
														],
														"properties": {
															"type": {
																"type": "string",
																"enum": [
																	"MultiPolygon"
																]
															},
															"coordinates": {
																"type": "array",
																"items": {
																	"type": "array",
																	"items": {
																		"type": "array",
																		"minItems": 4,
																		"items": {
																			"type": "array",
																			"minItems": 2,
																			"items": {
																				"type": "number"
																			}
																		}
																	}
																}
															},
															"bbox": {
																"type": "array",
																"minItems": 4,
																"items": {
																	"type": "number"
																}
															}
														}
													}
												]
											}
										},
										"bbox": {
											"type": "array",
											"minItems": 4,
											"items": {
												"type": "number"
											}
										}
									}
								}
							]
						},
						"bbox": {
							"type": "array",
							"minItems": 4,
							"items": {
								"type": "number"
							}
						}
					}
				}
			},
			"bbox": {
				"type": "array",
				"minItems": 4,
				"items": {
					"type": "number"
				}
			}
		}
	}, 
	
    "TargetStrengthMethod": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "type": "array",
        "items": [ 
            {
                "type": "object",
                "properties": {
                    "TargetStrengthMethod": { "type": "string" }
                },
                "required": [
                    "TargetStrengthMethod"
                ]
            }
        ]
    },
    "TargetStrengthTable": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "type": "array",
        "items": [ 
            {
                "type": "object",
                "properties": {
                    "AcousticCategory": { "type": "string" }, 
                    "Frequency": { "type": "number" },
                    "TargetStrength0": { "type": "number" },
                    "LengthExponent": { "type": "number" },
                    "DepthExponent": { "type": "number" },
                    "TargetStrength": { "type": "number" },
                    "TotalLength": { "type": "number" }
                },
                "required": [
                    "AcousticCategory",
                    "Frequency"
                ]
            }
        ]
    }, 







    
    "AcousticPSU": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "type": "object",
        "properties": {
            "Stratum_PSU": {
              "$ref": "#/Stratum_PSU"
            },
            "EDSU_PSU": {
              "$ref": "#/EDSU_PSU"
            },
            "PSUByTime": {

    "BioticPSU": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "type": "object",
        "properties": {
            "Stratum_PSU": {
              "$ref": "#/Stratum_PSU"
            },
            "EDSU_PSU": {
              "$ref": "#/EDSU_PSU"
            }
        },
        "required": [
            "Stratum_PSU",
            "Station_PSU"
        ]
    }, 	
    
    "AcousticTargetStrength": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "type": "object",
        "properties": {
            "TargetStrengthMethod": {
              "$ref": "#/TargetStrengthMethod"
            },
            "TargetStrengthTable": {
              "$ref": "#/TargetStrengthTable"
            }
        },
        "required": [
            "TargetStrengthMethod",
            "TargetStrengthTable"
        ]
    }
}

{
 
    "Stratum_PSU": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "type": "array",
        "items": [ 
            {
                "type": "object",
                "properties": {
                    "Stratum": { "type": "string" },
                    "PSU": { "type": "string" }
                },
                "required": [
                    "Stratum",
                    "PSU"
                ]
            }
        ]
    },
    "EDSU_PSU": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "type": "array",
        "items": [ 
            {
                "type": "object",
                "properties": {
                    "EDSU": { "type": "string" },
                    "PSU": { "type": "string" }
                },
                "required": [
                    "EDSU",
                    "PSU"
                ]
            }
        ]
    },
    "PSUByTime": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "type": "array",
        "items": [ 
            {
                "type": "object",
                "properties": {
                    "Stratum": { "type": "string" },
    		        "PSU": { "type": "string" },
    		        "Cruise": { "type": "string" },
    		        "StartDateTime": { "type": "string" },
    		        "StopDateTime": { "type": "string" }
                },
                "required": [
                    "Stratum",
    		        "PSU",
    		        "Cruise",
    		        "StartDateTime",
    		        "StopDateTime"
                ]
            }
        ]
    },
    
    "Station_PSU": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "type": "array",
        "items": [ 
            {
                "type": "object",
                "properties": {
                    "Station": { "type": "string" },
                    "PSU": { "type": "string" }
                },
                "required": [
                    "Station",
                    "PSU"
                ]
            }
        ]
    },
        
    "BioticLayer": {
		"$schema": "https://json-schema.org/draft/2020-12/schema",
		"type": "array",
		"items": [
			{
				"type": "object",
				"properties": {
				    "Layer": { "type": "string" }, 
                    "MinLayerDepth": { "type": "number" }, 
                    "MaxLayerDepth": { "type": "number" }
				},
				"required": [
					"Layer",
					"MinLayerDepthMinLayerDepth",
					"MaxLayerDepth"
				]
			}
		]
	}, 

    "AcousticLayer": {
		"$schema": "https://json-schema.org/draft/2020-12/schema",
		"type": "array",
		"items": [
			{
				"type": "object",
				"properties": {
				    "Layer": { "type": "string" }, 
                    "MinLayerDepth": { "type": "number" }, 
                    "MaxLayerDepth": { "type": "number" }
				},
				"required": [
					"Layer",
					"MinLayerDepthMinLayerDepth",
					"MaxLayerDepth"
				]
			}
		]
	}, 

    "BioticAssignment": {
		"$schema": "https://json-schema.org/draft/2020-12/schema",
		"type": "array",
		"items": [
			{
				"type": "object",
				"properties": {
					"Stratum": { "type": "string" },
					"PSU": { "type": "string" },
					"Layer": { "type": "string" },
					"Haul": { "type": "string" },
					"WeightingFactor": { "type": "number" }
				},
